{
    "name": "rela-scripting-lang",
    "displayName": "Rela Scripting Lang",
    "description": "Syntax highlighter for the Rela Scripting Language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.39.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "rela",
            "aliases": ["Rela Script", "rela"],
            "extensions": [".rela"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "rela",
            "scopeName": "source.rela",
            "path": "./syntaxes/rela.tmLanguage.json"
        }]
    }
}